{
	"manifest_version": 2,
	
	"name": "Read Later",
	"description": "This extension notifies you to view a page later", 
	"version": "0.1",

	"browser_action": {
		"default_title": "ReadLater",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"activeTab",
		"bookmarks"
	]
}
